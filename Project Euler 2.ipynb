    "import numpy as np\n",
    "\n",
    "def fibnummax(n):\n",
    "    num_list = [1,2]\n",
    "    while num_list[-1] + num_list[-2] <= n:\n",
    "        num_list.append(num_list[-1] + num_list[-2])\n",
    "    return num_list\n",
    "\n",
    "addends = fibnummax(4000000)\n",
    "for i in addends:\n",
    "    sum = 0\n",
    "    if i % 2 == 0:\n",
    "        sum += i\n",
    "\n",
    "print(sum)"
   ]
