      "source": [
        "def palindrome(n):\n",
        "  return str(n) == str(n)[::-1]\n",
        "\n",
        "palindrome_product = 0\n",
        "for i in range (100,1000):\n",
        "  for j in range (100,1000):\n",
        "    a = i*j\n",
        "    if palindrome(a) and a > palindrome_product:\n",
        "      palindrome_product = a\n",
        "\n",
        "print(palindrome_product)"
      ]
    }
  ]
}